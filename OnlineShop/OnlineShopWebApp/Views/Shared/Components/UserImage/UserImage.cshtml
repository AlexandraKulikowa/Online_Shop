@model string

@if(Model == null)
{
    <a class="btn entered" asp-area="" asp-controller="account" asp-action="profile" asp-route-name="@User.Identity.Name">Личный кабинет</a>
}
else
{
    <a class="btn entered-circle" asp-area="" asp-controller="account" asp-action="profile" asp-route-name="@User.Identity.Name">
        <img src="@Model" alt="avatar" height="30" class="rounded-circle">
    </a>
}